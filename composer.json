{
  "name": "zhishiq/flarum-redis",
  "description": "Allow user to bind with wechat",
  "type": "flarum-extension",
  "keywords": [
    "redis"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bill Lu",
      "email": "lubobill1990@163.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ZhiShiQ/flarum-redis/issues",
    "source": "https://github.com/ZhiShiQ/flarum-redis"
  },
  "require": {
    "flarum/core": "^0.1.0-beta.8",
    "illuminate/redis": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "ZhiShiQ\\Flarum\\Redis\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "Redis Connector",
      "icon": {
        "image": "redis.svg",
        "backgroundSize": "46% 63%",
        "backgroundPosition": "center",
        "backgroundRepeat": "no-repeat",
        "backgroundColor": "#fff",
        "color": "#fff"
      }
    }
  }
}
